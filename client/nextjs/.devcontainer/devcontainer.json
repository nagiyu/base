{
  "name": "nextjs",
  "image": "node:20",
  "remoteUser": "node",
  "containerEnv": {
    "PROCESS_ENV": "local",
    "CLIENT_NEXTJS_TEMPLATE_AWS_ACCESS_KEY": "",
    "CLIENT_NEXTJS_TEMPLATE_AWS_SECRET_ACCESS_KEY": "",
    "CLIENT_NEXTJS_TEMPLATE_AWS_REGION": ""
  },
  "workspaceFolder": "/workspaces/nagiyu-template/client/nextjs/nextjs.code-workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "donjayamanne.githistory",
        "eamodio.gitlens",
        "github.vscode-pull-request-github",
        "github.copilot",
        "GitHub.copilot-chat",
        "saoudrizwan.claude-dev",
        "oderwat.indent-rainbow",
        "wmaurer.change-case",
        "humao.rest-client",
        "bierner.markdown-mermaid",
        "wayou.vscode-todo-highlight",
        "dbaeumer.vscode-eslint"
      ]
    }
  }
}